{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 *
 * @see ./core/themes/stable/templates/navigation/menu-local-tasks.html.twig
 */
#}
{% if primary %}
  {% include '@oe_theme/compositions/ec-component-navigation/local-tasks.html.twig' with {
    title: 'Primary tabs'|t,
    links: primary_links
  } %}
{% endif %}
{% if secondary %}
  {% include '@oe_theme/compositions/ec-component-navigation/local-tasks.html.twig' with {
    title: 'Secondary tabs'|t,
    links: secondary_links,
    variant: 'secondary'
  } %}
{% endif %}
