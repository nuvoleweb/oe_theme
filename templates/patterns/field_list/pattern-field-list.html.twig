{#
/**
 * @file
 * Field list default variant.
 */
#}

{% set _items = [] %}
{% for _item in items %}
  {% set _items = _items|merge([{
    term: _item.label,
    definition: _item.body|render
  }]) %}
{% endfor %}

{% include '@ecl-twig/description-list' with {
  'variant': variant,
  'items': _items,
} %}
